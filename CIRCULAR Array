import java.util.Scanner;
import java.util.NoSuchElementException;

public class circularArray<T> {
    private final static int default = 5;
    private int front, rear, count;
    private T[] queue;
    // constructor
   public CircullarArray(int initialize) {
       front = rear = count = 0;
        queue = (T[]) new Object[initialize];
    }
    public CircullarArray() {
        this(default);
    }

    //size-ka
    public int size() {
        return count;
    }

    public boolean isEmpty() {
        return count == 0;
    }

    void Add (T element) {
        if (size() == queue.length) {
            throw new NoSuchElementException("Queue is full");
        }
        if (isDuplicate(element)){
            System.out.println("This element already in use");
            return;
        }
        queue[rear] = element;
        rear = (rear + 1) % queue.length;
        count++;
    }

    boolean isDuplicate(T element){
        int index = front;
        for (int i =0; i< count; i++) {
            if (queue[index] == element)
                return true;
            index = (index + 1) % queue.length;
        }
        return false;
    }
    public T Add() {
        if (isEmpty())
            throw new NoSuchElementException("Queue is Empty");
        T out = queue[front];
        queue[front] =null;
        front = (front + 1) % queue.length;
        count--;
        return out;
    }

     T first() {
       if (isEmpty())
           throw new NoSuchElementException("Queue is Empty");
       return queue[front];
    }
    void SearchArray(T value) {
        int index = front;
        for (int i= 0; i < count; i++){
            if (queue[index] == value){
                System.out.println("Found the element at position:"+ index);
                return;
            }
            index = (index+1) % queue.length;
        }
        System.out.println("Element not Found");
    }
    void print() {
        if (isEmpty())
            throw new NoSuchElementException("Queue is Empty");
        int index = front;
        for (int i = 0; i < count; i++) {
            System.out.println(queue[index] + " ");
            index = (index + 1) % queue.length;
        }
        System.out.println();
    }

}



public class Display {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("Enter the Queue Array Size: ");
        int Size = input.nextInt();
        CircullarArray<Integer> queue = new CircullarArray<Integer>(6);
        queue.Add(90);
        queue.Add(13);
        queue.Add(77);
        System.out.println("The Queue List");
        queue.print();
        System.out.println("The Queue Size: "+ queue.size());
        queue.Remove();
        queue.SearchA rray(13);
        System.out.println("The Queue List after remove: ");
        queue.print();
    }
}
